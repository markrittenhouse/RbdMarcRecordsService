<?xml version="1.0" encoding="utf-8" ?>
<theme>
  <name>Dark Gray Theme</name>
  <global>
    <!--Use HTML web color codes for these values-->
    <font_color>#ffffff</font_color>
    <background_color>#808080</background_color>
  </global>
  <marceditor>
    <font_color>#000000</font_color>
    <background_color>#ffffff</background_color>
  </marceditor>
  <overrides>
    <!--
      Override values
      <menus>
         <font_color>         
         <background_color>
      </menus>
      <links>
         <font_color>
         <visited_font_color>
         <behavior> [set to always, hover, none]
      </links>
    -->
  </overrides>
</theme>
